;;AACA,WAAA,KAAA,CAAA,YAAA,CAAA,QAAA;AAAA,2EAAA,iBAAA,KAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAAA;AAAA,yBAGA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA,EAAA,KAAA,CAAA,SAAA,CAHA;;AAAA;AAAA;AAGA,kBAAA,GAHA,yBAGA,IAHA;;AAAA,wBAIA,GAAA,CAAA,MAAA,IAAA,IAJA;AAAA;AAAA;AAAA;;AAKA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;AACA,kBAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,MAAA,EAAA,GAAA,CAAA,IAAA;;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA;;AARA;AAAA;;AAAA;AAUA,kBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,WAAA;;AAVA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAcA;AAnBA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login_container\">\r\n    <div class=\"login_box\">\r\n      <div class=\"avatar_box\">\r\n        <!--头像-->\r\n        <img src=\"../assets/bigMan.png\" alt />\r\n      </div>\r\n      <!--添加表单-->\r\n      <el-form\r\n        ref=\"loginFormRef\"\r\n        :model=\"loginForm\"\r\n        :rules=\"loginRules\"\r\n        class=\"login_form\"\r\n        label-width=\"0px\"\r\n      >\r\n        <el-form-item prop=\"username\">\r\n          <el-input v-model=\"loginForm.username\" prefix-icon=\"iconfont icon-denglu\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input v-model=\"loginForm.password\" prefix-icon=\"iconfont icon-denglumima\" type=\"password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item class=\"btns\">\r\n          <el-button type=\"primary\" @click=\"login\">登录</el-button>\r\n          <el-button type=\"info\" @click=\"resetLoginForm\">重置</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      loginForm: {\r\n        username: \"admin\",\r\n        password: \"123456\"\r\n      },\r\n      loginRules: {\r\n        username: [\r\n          { required: true, message: \"请输入用户名\", trigger: \"blur\" },\r\n          { min: 5, max: 8, message: \"长度在 5 到 8 个字符\", trigger: \"blur\" }\r\n        ],\r\n        password: [\r\n          { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n          { min: 6, max: 8, message: \"密码为 6~8 位\", trigger: \"blur\" }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    resetLoginForm() {\r\n      this.$refs.loginFormRef.resetFields();\r\n    },\r\n    login() {\r\n      this.$refs.loginFormRef.validate(async valid => {\r\n        if (!valid) return;\r\n        // 调用get请求\r\n        const{data: res} = await this.$http.post(\"login\", this.loginForm);\r\n        if(res.status == \"OK\"){\r\n          console.log(res.user)\r\n          window.sessionStorage.setItem(\"user\", res.user);\r\n          this.$message.success(\"登陆成功！\");\r\n          this.$router.push({path: '/home'});\r\n        }else{\r\n          this.$message.error(\"用户身份认证失败！\");\r\n          return;\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n// 跟节点样式\r\n.login_container {\r\n  background-color: #2b4b6b;\r\n  height: 100%;\r\n}\r\n// 登录框\r\n.login_box {\r\n  width: 450px;\r\n  height: 300px;\r\n  background-color: #fff;\r\n  border-radius: 3px;// 圆角\r\n  position: absolute;// 绝对定位\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);// 根据自己位置 以自己为长度位移\r\n  // 头像框\r\n  .avatar_box {\r\n    width: 130px;\r\n    height: 130px;\r\n    border: 1px solid #eee;\r\n    border-radius: 50%; // 加圆角\r\n    padding: 10px;\r\n    box-shadow: 0 0 10px #ddd;// 盒子阴影\r\n    position: absolute;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: #4082ff;\r\n    img {\r\n      width: 100%;\r\n      height: 100%;\r\n      border-radius: 50%; // 加圆角\r\n      background-color: #eee;\r\n    }\r\n  }\r\n  .btns {\r\n    display: flex;// 弹性布局\r\n    justify-content: flex-end; // 尾部对齐\r\n  }\r\n  .login_form {\r\n    position: absolute;\r\n    bottom: 0%;\r\n    width: 100%;\r\n    padding: 0 10px;\r\n    box-sizing: border-box;// 设置边框\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}